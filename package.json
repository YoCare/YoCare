{
  "name": "yocare",
  "version": "1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "push": "npm build && copy .\\dist\\index.html .\\dist\\200.html && surge --project ./dist/ --domain yocare.surge.sh"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "@ssthouse/vue-tree-chart": "^0.6.5",
    "core-js": "^2.6.5",
    "sweetalert2": "^11.3.10",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.10",
    "vue-material-design-icons": "^5.0.0",
    "vue-navigation-bar": "^4.1.0",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "surge": "^0.23.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "prettier/prettier": 0,
      "no-unused-vars": 1,
      "no-empty": 1,
      "no-console": 1,
      "vue/no-unused-components": 1
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
